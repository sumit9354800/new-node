<%- include('../partial/head') %>
  </head>

  <body>

    <%- include('../partial/navbar') %>
      <div class="flex justify-center flex-col items-center w-full my-20">
        <h1 class="text-2xl font-bold mb-5 text-red-500">
          Login Airbnb
        </h1>
        <form action="/login" method="post" class="flex w-4/12 flex-col gap-5">
          <input class="w-full bg-red-300 px-3 py-2 rounded-md" type="text" name="username" placeholder="Enter username"
            required>
          <input class="w-full bg-red-300 px-3 py-2 rounded-md" type="password" name="password"
            placeholder="Enter Password" required>
          <input class="w-full bg-red-500 text-white px-3 py-2 rounded-md" type="submit">
        </form>
      </div>

  </body>

  </html>